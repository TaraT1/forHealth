<section class="container-fluid container-fluid-custom pb-5 mb-5">
    <div class="row mt-5">
        <div class="col col-lg-6 mx-auto">
            <h2>Create a Provider</h2>

            <div>

                <form action="/providers" method="POST" enctype="multipart/form-data">
                    
                    <%- include("_form_fields") %>

                    <div class="d-flex justify-content-between pt-3">
                        <a type="text" class="btn btn-secondary btn-lg"
                        href="/providers">Cancel</a>
                        <button type="submit" class="btn btn-primary btn-lg">Create</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>

